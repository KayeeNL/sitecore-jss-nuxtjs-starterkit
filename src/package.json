{
  "name": "@uniformdev/uniform-sitecore-jss-nuxtjs-starterkit",
  "version": "1.0.0",
  "private": true,
  "config": {
    "appName": "uniform-jss",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/api/uniform-jss",
    "language": "en"
  },
  "scripts": {
    "start": "cross-env-shell JSS_MODE=connected \"npm-run-all --serial bootstrap:connected --parallel start:dev watch-components\"",
    "start:withtunnel": "cross-env-shell JSS_MODE=connected \"npm-run-all --serial bootstrap:connected --parallel start:dev:withtunnel watch-components\"",
    "start:production": "cross-env-shell JSS_MODE=connected NODE_ENV=production \"npm-run-all --serial bootstrap:connected nuxt:build start:server",
    "start:dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "start:dev:withtunnel": "cross-env NODE_ENV=development nodemon server/tunnel.js --watch server --ignore server/tunnel.js",
    "nuxt:build": "nuxt build",
    "start:server": "node server/index.js",
    "watch-components": "node scripts/generate-component-factory.js --watch",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "nuxt:generate": "cross-env NUXT_EXPORT=true nuxt generate",
    "export": "npm-run-all --serial jssconfig bootstrap:connected nuxt:generate",
    "jssconfig": "node ./scripts/create-scjssconfig.js",
    "azure": "node ./deploy-azure.js",
    "aws": "node ./deploy-aws.js",
    "netlify": "node ./deploy-netlify.js",
    "deploy:azure": "npm-run-all --serial bootstrap:connected nuxt:generate azure",
    "deploy:aws": "npm-run-all --serial bootstrap:connected nuxt:generate aws",
    "deploy:netlify": "npm-run-all --serial bootstrap:connected nuxt:generate netlify",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.2.0",
    "@sitecore-jss/sitecore-jss-tracking": "~12.0.0",
    "@sitecore-jss/sitecore-jss-vue": "~12.0.0",
    "@uniformdev/nuxt-server": "^2.0.200328-1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "follow-redirects": "^1.10.0",
    "loglevel": "^1.6.7",
    "i18next": "^19.3.2",
    "nuxt": "^2.11.0",
    "set-cookie-parser": "^2.4.3",
    "vue-loading-overlay": "^3.3.2"
  },
  "devDependencies": {
    "@babel/register": "^7.8.0",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/tailwindcss": "^1.4.0",
    "@sitecore-jss/sitecore-jss-cli": "^12.0.0",
    "@sitecore-jss/sitecore-jss-dev-tools": "^12.0.0",
    "@sitecore-jss/sitecore-jss-manifest": "^12.0.0",
    "@uniformdev/publishing-awss3": "^2.0.200328-1",
    "@uniformdev/publishing-azureblobstorage": "^2.0.200328-1",
    "@uniformdev/publishing-netlify": "^2.0.200328-1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "chokidar": "^3.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^25.1.0",
    "ngrok": "^3.2.7",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "vue-jest": "^4.0.0-0",
    "webpack-require-from": "^1.8.1"
  }
}
